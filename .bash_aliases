## get rid of command not found
alias cd..='cd ..'

## a quick way to get out of current directory
alias ..='cd ..'
alias .2='cd ../..'
alias .3='cd ../../..'
alias .4='cd ../../../..'
alias .5='cd ../../../../..'

alias e="emacs -nw"
alias em="emacs -nw"

alias cat="batcat"
alias bat="batcat"

# editor
export EDITOR=emacs
export GIT_EDITOR=emacs
